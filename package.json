{
  "name": "zyk-cli",
  "version": "1.0.6",
  "description": "生成 React 或 Vue初始代码，生成 webpack 配置，以及配置 webpack 优化项",
  "main": "./dist/bin/index.js",
  "module": "./dist/bin/index.js",
  "bin": {
    "zyk": "./dist/bin/index.js"
  },
  "scripts": {
    "build": "rimraf dist && rollup --config rollup.config.ts --configPlugin typescript"
  },
  "keywords": [
    "Webpack",
    "Rollup",
    "TypeScript",
    "React",
    "Vue"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4",
    "commander": "^9.4.1",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.0.0",
    "ora": "^5.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.0.0",
    "@types/node": "^18.11.9",
    "rimraf": "^3.0.2",
    "rollup": "^3.3.0",
    "rollup-plugin-copy": "^3.4.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zyk987/zyk-cli.git"
  }
}
